Difficulty:  284
Pblm Name - Best of Two
---------------Logic----------------
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int x,y;
	    cin>>x>>y;
	    if(x>y)
	    {
	        cout<<x<<endl;
	    }
	    else
	    {
	        cout<<y<<endl;
	    }
	}
	return 0;
}
