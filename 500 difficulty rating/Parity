Pblm Name -Parity
-------------Logic-------------------
#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    while(n--){
        int a;
        cin>>a;
        if(a%2==0)
        cout<<"Yes"<<endl;
        else
        cout<<"No"<<endl;
    }
	return 0;
}
