Prb Name - Greater Average
-----------------------------------------------
#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    float a,b,c,d;
	    cin>>a>>b>>c;
	    d = (a+b)/2;
	    if(d>c)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
	

}
